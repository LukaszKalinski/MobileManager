<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ActivityMain"></activity>
        <activity android:name=".ActivityCalendar"></activity>
        <activity android:name=".ActivityFinance"></activity>
        <activity android:name=".ActivityMessages"></activity>
        <activity android:name=".ActivityStadium"></activity>
        <activity android:name=".ActivityTeam"></activity>
        <activity android:name=".Profile"></activity>
        <activity android:name=".ActivityResults"></activity>


    </application>

</manifest>